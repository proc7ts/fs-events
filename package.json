{
  "name": "@proc7ts/fs-events",
  "version": "1.0.0-dev.0",
  "description": "Functional file system events processor",
  "keywords": [
    "fs.watch",
    "fsevents",
    "reactive-events"
  ],
  "homepage": "https://github.com/proc7ts/fs-events",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:proc7ts/fs-events.git"
  },
  "license": "MIT",
  "author": "Ruslan Lopatin <ruslan.lopatin@gmail.com>",
  "bugs": {
    "url": "https://github.com/proc7ts/fs-events/issues"
  },
  "type": "module",
  "main": "./dist/fs-events.js",
  "types": "./index.d.ts",
  "exports": "./dist/fs-events.js",
  "engines": {
    "node": ">= 14.16"
  },
  "peerDependencies": {
    "@proc7ts/fun-events": "^10.5.1"
  },
  "dependencies": {
    "@proc7ts/delta-set": "^1.5.0",
    "@proc7ts/supply": "^1.2.3"
  },
  "devDependencies": {
    "@jest/globals": "^27.5.1",
    "@proc7ts/fun-events": "^10.5.1",
    "@proc7ts/primitives": "^3.0.2",
    "@run-z/eslint-config": "^3.0.0",
    "@run-z/rollup-helpers": "^2.0.0",
    "@types/node": "^14.18.16",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-plugin-jest": "^26.1.5",
    "gh-pages": "^3.2.3",
    "jest": "^27.5.1",
    "jest-junit": "^13.2.0",
    "jest-mock": "^27.5.1",
    "prettier": "^2.6.2",
    "rollup": "^2.71.0",
    "rollup-plugin-flat-dts": "^1.4.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.31.2",
    "run-z": "^1.10.0",
    "shx": "^0.3.4",
    "ts-jest": "^27.1.4",
    "tslib": "^2.4.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "build": "run-z +z --then rollup -c",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then shx rm -rf \"index.d.ts?(.map)\" dist target",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "format": "run-z +z --then prettier --write \"**/*.{json,md}\"",
    "lint": "run-z +z --then eslint .",
    "test": "run-z +z env:NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" --then jest",
    "z": "run-z +cmd:rollup,+cmd:typedoc,+cmd:eslint,+cmd:jest"
  }
}
