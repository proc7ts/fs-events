  "bugs": {
    "url": "https://github.com/proc7ts/fs-events/issues"
  },
  "type": "module",
  "types": "./dist/fs-events.d.ts",
  "exports": {
    "types": "./dist/fs-events.d.ts",
    "default": "./dist/fs-events.js"
  },
  "engines": {
    "node": ">= 16.15"
  },
  "peerDependencies": {
    "@proc7ts/fun-events": "^10.5.1"
  },
  "dependencies": {
    "@proc7ts/delta-set": "^2.0.0",
    "@proc7ts/supply": "^1.2.3"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@proc7ts/fun-events": "^10.5.2",
    "@proc7ts/primitives": "^3.0.2",
    "@run-z/eslint-config": "^3.3.1",
    "@run-z/prettier-config": "^2.0.0",
    "@run-z/project-config": "^0.15.4",
    "@swc/core": "^1.3.51",
    "@swc/jest": "^0.2.26",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-plugin-jest": "^27.2.1",
    "gh-pages": "^5.0.0",
    "jest": "^29.5.0",
    "jest-junit": "^15.0.0",
    "jest-mock": "^29.5.0",
    "prettier": "^2.8.7",
    "prettier-eslint-cli": "^7.1.0",
    "rollup": "^3.20.3",
    "run-z": "^1.11.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.0",
    "typedoc": "^0.24.4",
    "typescript": "~5.0.4"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "build": "run-z +z --then build-z",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then clean-z",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "format": "run-z +z --then prettier-eslint --write --include-dot-files \"src/**/*.*\" \"*.{js,cjs,json,md}\"",
    "lint": "run-z +z --then eslint .",
    "test": "run-z +z env:NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" --then test-z",
    "z": "run-z +cmd:build-z,+cmd:typedoc,+cmd:eslint,+cmd:test-z"
  }
}
